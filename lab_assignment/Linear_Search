#include<iostream>
using namespace std;
int main(){
    int n,i,key;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    cin>>key;
    for(int i=0;i<n;i++){
        if(arr[i] == key){
            cout<<"Index : "<<i<<endl;
            cout<<"Comparision : "<<i+1<<endl;
            exit(0);
        }
    }
    cout<<"Comparisions : "<<n<<endl;
    return 0;
}
